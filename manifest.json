{
  "manifest_version": 3,
  "name": "Write Your NewTab",
  "version": "0.1.0",
  "description": "Replace the default new tab page with your own custom HTML, CSS, and JavaScript",
  "permissions": ["storage"],
  "action": {
    "default_title": "Write Your NewTab"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "view/newtab.html"
  }
}
